{
  "extends": "./node_modules/aurelia-tools/.eslintrc.json",
  "rules": {
    "space-before-function-paren": [0, "never"],
    "space-before-blocks": [ 0, "never"],
    "no-trailing-spaces": [ 1, {"skipBlankLines" : true}],
    "no-unused-vars": [0, "never"],
    "no-console": [0, "never"],
    "no-alert": [0, "never"],
    "no-empty": [0, "never"]
  }
}
